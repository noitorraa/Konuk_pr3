<Page x:Class="Konuk_pr3.Pages.Director"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Konuk_pr3.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Director">

    <Grid>
        <TextBlock x:Name="tbFIO" HorizontalAlignment="Left" Margin="374,97,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="tbTime" HorizontalAlignment="Left" Margin="374,61,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>

        <FlowDocumentReader Grid.Row="0">
            <FlowDocument Name="flowDoc">
                <BlockUIContainer>
                    <StackPanel HorizontalAlignment="Center"/>
                </BlockUIContainer>
            </FlowDocument>
        </FlowDocumentReader>
        <Button x:Name="butPrint" Content="Печать" HorizontalAlignment="Left" Margin="366,218,0,0" VerticalAlignment="Top" Width="75" Click="butPrint_Click"/>
    </Grid>
</Page>
